<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
  <display-name>Archetype Created Web Application</display-name>

  <!-- Servlets -->
  <servlet>
    <servlet-name>Ragna</servlet-name>
    <servlet-class>com.servletexample.Information</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Ragna</servlet-name>
    <url-pattern>/Rock</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Ragna1</servlet-name>
    <servlet-class>com.servletexample.Information2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Ragna1</servlet-name>
    <url-pattern>/ragna</url-pattern>
  </servlet-mapping>
</web-app>
